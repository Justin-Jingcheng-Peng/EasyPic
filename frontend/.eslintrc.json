{
    "plugins": ["react", "prettier"],
    "extends": ["react-app", "airbnb", "prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": false
        }
      ],
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "react/react-in-jsx-scope": "off"
    }
  }
